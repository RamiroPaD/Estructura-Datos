Lista_Nodo

/**
 *La clase Nodo acepta cualquier tipo de valores y tiene constructores
 * @author Ramiro Padierna Delgado
 * @author 1224100710.rpd@gmail.com
 */
public class Nodo<T> {
    private T dato;
    private Nodo izquieda;

    public Nodo getIzquieda() {
        return izquieda;
    }

    public void setIzquieda(Nodo izquieda) {
        this.izquieda = izquieda;
    }
    
    public T getDato() {
        return dato;
    }

    public void setDato(T dato) {
        this.dato = dato;
    }

    public Nodo(){
    
    }
    
    
}
