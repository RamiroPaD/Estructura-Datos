        public void recorrer(){
            Nodo<T> temp= cabeza;
            while(temp.getIzq() !=null){
                System.out.println(temp.getDato());
                temp = temp.getIzq();
            }
        }
        public int inspeccionar(){
            return 0;
        }

        public void insertar(Nodo node){
            
        }
        
        public boolean atender(){
           return true; 
           
        }
    }
